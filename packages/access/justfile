export RUSTFLAGS := "-C strip=none" 

target := "../../target"

build:
    cargo +nightly-2024-11-22 build --target=wasm32-unknown-unknown --release --features certora

expand:
    cargo +nightly-2024-11-22 expand --target=wasm32-unknown-unknown --release --features certora

clean:
    rm -rf {{target}}